generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DB_CONN_STRING")
}

model KOL {
  id String @map("_id") @default(cuid())
  handle String
  aurarank Int
  pfp String
  category Categories
}

enum Categories {
  CELEB
  ATHLETE
  ACTOR
  CRYPTO
  MOVIES
}